<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.krrishdholakia.projectapollox2v10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".App_Sign_In.ChooserActivity"
            android:label="Choosing">
        </activity>

        <activity android:name=".App_Sign_In.GoogleSignInActivity"
            android:label="Google">
        </activity>

        <activity android:name=".App_Sign_In.EmailPasswordActivity"
            android:label="EmPas">

        </activity>

        <activity android:name=".App_Sign_In.AnonymousAuthActivity"
            android:label="Anon">
        </activity>

        <activity android:name=".App_Sign_In.CustomAuthActivity"
            android:label="CustAuth">

        </activity>

    </application>

</manifest>